<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/veiculos"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="412dp"
        android:layout_height="53dp"
        android:layout_marginTop="36dp"
        android:background="#673AB7"
        android:text="Meus Veiculos"
        app:layout_constraintTop_toBottomOf="@+id/obs"
        tools:layout_editor_absoluteX="-1dp" />

    <Button
        android:id="@+id/Dados"
        style="313dp"
        android:layout_width="414dp"
        android:layout_height="45dp"
        android:background="#673AB7"
        android:text="Meus Dados"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/veiculos"
        app:layout_constraintVertical_bias="0.11"
        tools:layout_editor_absoluteX="1dp" />

    <Button
        android:id="@+id/Notas"
        android:layout_width="415dp"
        android:layout_height="50dp"
        android:layout_marginTop="40dp"
        android:background="#673AB7"
        android:text="Notas"
        app:layout_constraintTop_toBottomOf="@+id/Manifests"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/Manifests"
        android:layout_width="412dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:background="#673AB7"
        android:text="Manifestos"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="1dp" />

    <Button
        android:id="@+id/obs"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="416dp"
        android:layout_height="55dp"
        android:layout_marginTop="28dp"
        android:background="#673AB7"
        android:text="Observações"
        app:layout_constraintTop_toBottomOf="@+id/Notas"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>